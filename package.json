{
  "name": "deepfilternet3-workers",
  "version": "1.0.0",
  "description": "Deepfilternet3 workers",
  "type": "module",
  "files": [
    "dist/worker/",
    "dist/models/",
    "dist/pkg/"
  ],
  "scripts": {
    "build": "rollup -c && yarn copy-assets",
    "copy-assets": "cp -r models dist/ && cp -r pkg dist/",
    "postbuild": "rm -f dist/worker/*.d.ts dist/worker/*.d.ts.map dist/worker/*.js.map dist/**/*.map && rm -rf dist/main || true",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "audio",
    "streaming",
    "noise-filter",
    "deepfilternet3",
    "audio-processing"
  ],
  "author": "phuvinh010701",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^20.0.0",
    "rollup": "^4.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phuvinh010701/deepfilternet3-workers.git"
  },
  "bugs": {
    "url": "https://github.com/phuvinh010701/deepfilternet3-workers/issues"
  },
  "homepage": "https://github.com/phuvinh010701/deepfilternet3-workers#readme"
}
